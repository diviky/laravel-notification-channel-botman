{
    "name": "diviky/laravel-notification-channel-botman",
    "description": "Use botman as laravel notification service",
    "homepage": "https://github.com/diviky/laravel-notification-channel-botman",
    "license": "MIT",
    "require": {
        "illuminate/notifications": "^5|^6|^7|^8",
        "illuminate/support": "^5|^6|^7|^8",
        "illuminate/events": "^5|^6|^7|^8"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\BotmanDriver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\BotmanDriver\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\BotmanDriver\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
